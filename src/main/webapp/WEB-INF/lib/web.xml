     </servlet>
         <servlet-mapping>
    	<servlet-name>DeleteCustomerServlet</servlet-name>
    	<url-pattern>/DeleteCustomerServlet</url-pattern>
    </servlet-mapping>
    
           <servlet>
    	<servlet-name>customerinsert</servlet-name>
    	<servlet-class>com.customer.CustomerInsert</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>customerinsert</servlet-name>
